  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>

  <!-- FAVICON -->
	<link rel="icon" type="image/png" href="/img/favicon.png" />

	<!-- IOS -->
	<meta name="apple-mobile-web-app-title" content="Jeff Batterton">
